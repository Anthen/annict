{
  "presets": [
    "@babel/typescript",
    [
      "@babel/preset-env",
      {
        "modules": false,
        "targets": {
          "browsers": ["last 2 versions", "Chrome >= 41"]
        },
        "useBuiltIns": "usage",
        "forceAllTransforms": true
      }
    ]
  ],

  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ]
}
